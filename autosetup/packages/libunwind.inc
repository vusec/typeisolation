echo "building libunwind"
cd "$PATHAUTOPACKSRC"
[ -f "$VERSIONLIBUNWINDURL.tar.gz" ] || run wget "https://github.com/libunwind/libunwind/archive/$VERSIONLIBUNWINDURL.tar.gz"
[ -d "$VERSIONLIBUNWIND" ] || run tar xf "$VERSIONLIBUNWINDURL.tar.gz"
run mkdir -p "$PATHAUTOPACKOBJ/$VERSIONLIBUNWIND"
cd "$PATHAUTOPACKOBJ/$VERSIONLIBUNWIND"
[ -f Makefile ] || run "$PATHAUTOPACKSRC/$VERSIONLIBUNWIND/autogen.sh"
[ -f Makefile ] || run "$PATHAUTOPACKSRC/$VERSIONLIBUNWIND/configure"
run mkdir -p "$PATHAUTOPREFIX/usr"
rm -f "$PATHAUTOPREFIX/usr/local"
run ln -s "$PATHAUTOPREFIX" "$PATHAUTOPREFIX/usr/local"
run make -j"$JOBS"
run make install prefix="$PATHAUTOPREFIX/usr/local"
